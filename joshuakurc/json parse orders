from "RIOT_SHOPIFY"."SHOPIFY_RIOTSTORES"."ORDERS" o
,lateral flatten(input => fulfillments) fa // flattens the Fulfillment column
,lateral flatten(input=>fa.value:line_items) fa_line_items // flattens the Line Items Array within the Fulfillment Column
,lateral flatten(input=>fa_line_items.value:tax_lines) fa_line_items_tax_lines //flattens the tax lines array within the Line Items Array within the Fulfillment Column
,lateral flatten(input=>fa_line_items.value:properties) fa_line_items_properties//flattens the tax lines array within the Line Items Array within the Fulfillment Column
,lateral flatten(input => line_items) li // flattens the Line_Items column
,lateral flatten(input=>li.value:tax_lines) line_items_tax_lines //flattens the tax lines array within the Line Items Column
,lateral flatten(input=>li.value:properties) line_items_properties //flattens the properties array within the Line Items Column
,lateral flatten(input=>note_attributes) note_att //flattens the Note Attributes Column
,lateral flatten(input => payment_gateway_names) pay_gate_names //flattens the Payment Gateway Names Column
,lateral flatten(input => shipping_lines) shipping_lines //flattens the Shipping Lines Column
,lateral flatten(input=>shipping_lines.value:tax_lines) ship_tax_lines //flattens the tax lines array within the Shipping Lines Column
,lateral flatten(input=>tax_lines) tax_lines 


where LI_VARIANT_ID = FA_LI_VARIANT_ID
and LI_SKU = FA_LI_SKU
and LI_PRODUCT_ID = FA_LI_PRODUCT_ID

)
;



